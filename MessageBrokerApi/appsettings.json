{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Broker": {
    "Directory": "C:\\temp\\broker",
    "IncorrectFilesDirectory": "C:\\temp\\broker\\incorrect",
    "FileAgeThresholdMin": 5,
    "AdvancedMode": true,
    "TimeoutSec": 90,
    "ResponseCacheLifetimeSeconds": 30,
    "BackendUrl": "https://localhost:64172",
    "StorageType": "File"
  },
  "RunServices": {
    "RunMessageBrokerConsumer": true
  }
}
